{
  "public": true,
  "buildCommand": "node scripts/inject-secrets.js",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/", "destination": "/ui/index.html" },
    { "source": "/import", "destination": "/ui/import.html" }
  ],
  "headers": [
    {
      "source": "/ui/data/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
